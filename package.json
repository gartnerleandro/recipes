{
  "name": "recipes",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "android": "react-native run-android",
    "build-android-prod": "cd android && ./gradlew bundleRelease && cd ..",
    "pod": "cd ios && pod install && cd ..",
